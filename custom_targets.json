{
    "MAPLE_MINI": {
        "inherits": [
            "MCU_STM32_BAREMETAL"
        ],
        "core": "Cortex-M3",
        "extra_labels_add": [
            "STM32F1",
            "STM32F103xB"
        ],
        "macros_add": [
            "STM32F103xB",
            "HSE_VALUE=8000000"
        ],
        "config": {
            "clock_source": {
                "help": "Mask value : USE_PLL_HSE_EXTC (SYSCLK=72 MHz) | USE_PLL_HSE_XTAL (need HW patch) | USE_PLL_HSI (SYSCLK=64 MHz)",
