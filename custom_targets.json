                "macro_name": "CLOCK_SOURCE"
            },
            "clock_source_usb": {
                "value": "1",
                "macro_name": "CLOCK_SOURCE_USB"
            }
        },
        "default_lib": "small",
        "device_has_add": [
            "CAN",
            "SERIAL_ASYNCH",
            "FLASH",
            "USBDEVICE"
        ],
        "device_has_remove": [
            "LPTICKER"
        ],
        "bootloader_supported": true,
        "mbed_rom_start": "0x08005000",
        "mbed_rom_size": "110592"
    }
}
